{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "projectFolder": ".",
  "mainEntryPointFilePath": "./dist/types/index.d.ts",
  "compiler": {
		"tsconfigFilePath": "./tsconfig-dist.json"
	},
  "apiReport": {
    "enabled": true,
     "reportFolder": "./dist/"
  },
  "docModel": {
    "enabled": true,
    "apiJsonFilePath": "./dist/three-wideline.api.json"
  },
  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "./dist/index.d.ts"
  },
  "messages": {
    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning",
        "addToApiReportFile": false
      },
      "ae-forgotten-export": {
        "logLevel": "warning",
        "addToApiReportFile": false
      }
    }
  }
}
