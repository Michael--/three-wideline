{
  "name": "three-wideline",
  "version": "0.1.3",
  "description": "A three js line implementation",
  "author": "michael--",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michael--/three-wideline.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "dependencies": {
    "@react-three/fiber": "^7.0.24",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "three": "^0.135.0"
  },
  "scripts": {
    "prep-sample1": "echo 'import start from \"./sample1/index\"\nstart()' > src/index.tsx",
    "debug-sample1": "npm run prep-sample1 && vite --config src/sample1/vite.config.js --host",
    "build-sample1": "npm run prep-sample1 && vite build --config src/sample1/vite.config.js --base=./",
    "dist-rollup": "rollup --config rollup.config.js",
    "start": "npm run debug-sample1",
    "build": "npm run build-sample1",
    "dist": "npm run dist-rollup",
    "prepack": "npm run lint && npm run dist",
    "lint": "eslint --ext .tsx,.ts,.jsx,.js src/"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.3.0",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.37",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.11",
    "@types/react-select": "^5.0.1",
    "@types/three": "^0.135.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "@vitejs/plugin-react": "^1.1.3",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "copyfiles": "^2.4.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "prettier": "^2.5.1",
    "react-color": "^2.19.3",
    "react-scripts": "5.0.0",
    "react-select": "^5.2.1",
    "rollup": "^2.61.1",
    "rollup-plugin-dts": "^4.0.1",
    "rollup-plugin-string": "^3.0.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.4",
    "vite-raw-plugin": "^1.0.1",
    "vite-tsconfig-paths": "^3.3.17"
  }
}
