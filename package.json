{
  "name": "three-wideline",
  "version": "0.1.2",
  "description": "A three js line implementation",
  "author": "michael--",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Michael--/three-wideline.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "license": "MIT",
  "private": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {
    "@react-three/fiber": "^7.0.21",
    "three": "^0.135.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "raw-loader": "^4.0.2"
  },
  "scripts": {
    "start": "BROWSER=none react-scripts start",
    "build-sample": "react-scripts build",
    "build": "tsc --project tsconfig-dist.json",
    "dist": "npm run build && npm run copy-shader",
    "copy-shader": "copyfiles -f src/Wideline/shader/*.* dist/shader",
    "prepack": "npm run dist",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^12.20.37",
    "@types/react": "^17.0.37",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.11",
    "@types/react-select": "^5.0.1",
    "@types/three": "^0.134.0",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.6.0",
    "copyfiles": "^2.4.1",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "prettier": "^2.5.1",
    "react-color": "^2.19.3",
    "react-scripts": "4.0.3",
    "react-select": "^5.2.1",
    "typescript": "^4.5.2"
  }
}
